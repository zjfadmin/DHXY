<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.come.mapper.SpeciesMapper">

	<!-- 查询所有种类信息 -->
	<select id="getAllSpecies" resultType="org.come.entity.Species">
		select SPECIES_ID,
		RACE_ID,
		SEX,
		LOCALNAME from species
	</select>

